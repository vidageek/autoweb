<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN"
 "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">

<taglib>
	<tlibversion>1.0</tlibversion>
	<jspversion>1.1</jspversion>
	<shortname>http://vidageek.net/autoweb</shortname>
	<uri>http://vidageek.net/autoweb</uri>
	<info>Autoweb tags</info>
	<tag>
		<name>css</name>
		<tagclass>net.vidageek.autoweb.taglib.CssTag</tagclass>
		<info>Generates css tag with cache invalidation parameter</info>
		<bodycontent>empty</bodycontent>
		<attribute>
			<name>url</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
</taglib>